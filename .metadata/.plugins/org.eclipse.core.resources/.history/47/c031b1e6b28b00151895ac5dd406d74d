package models;

import java.util.ArrayList;

public class Category {

	public Category(String name) {
		this.name = name;
	}

	private String name;

	public String getName() {
		return name;
	}

	private ArrayList<Job> jobs=new ArrayList<Job>();
	
	public void add(Job job) {
		jobs.add(job);
	}

	public ArrayList<Job> getJobs() {
		return jobs;
	}
	
	
	
	
}
